--updates
function update_game()
 if btnp(⬅️) then
  p_x-=1
 end
 if btnp(➡️) then
  p_x+=1
 end
 if btnp(⬆️) then
  p_y-=1
 end
 if btnp(⬇️) then
  p_y+=1
 end
 
end

function update_gameover()

end
